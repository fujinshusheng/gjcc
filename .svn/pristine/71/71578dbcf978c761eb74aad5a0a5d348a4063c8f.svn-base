                        var _this = this;
                        if ((_this.shopdetails.FLimitNum > 0 && _this.shopdetails.FLimitNum > this
                                .number) || _this.shopdetails.FLimitNum == 0) {
                            if (this.number == "" || this.number == undefined) {
                                this.number = 1;
                            } else {
                                if (parseFloat(this.number) > parseFloat(_this.shopdetails.FLimitNum)) {
                                    jw.ts.toast("购买数量不能高于限购数量！");
                                    return false
                                }
                                this.number++;
                            }
                            this.getprice();
                        }
