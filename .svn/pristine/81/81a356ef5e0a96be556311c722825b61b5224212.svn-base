@charset "UTF-8";

html,
body {
    margin: 0;
    padding: 0;
    width: 100%;
    height: 100%;
}

.treebox {
    width: 100%;
    height: 100%;
    overflow: hidden;

    position: fixed;
    width: 100%;
    /*height: 666px;*/
    top: 50%;
    left: 50%;
    -webkit-transform: translate(-50%, -50%);
    transform: translate(-50%, -50%);
}

.treebg {
    position: absolute;
    top: -20px;
    left: -20px;
    right: -20px;
    bottom: -20px;
    background: url("img/treebj1.png") center center no-repeat;
    background-size: 100% auto;
    z-index: 1;
}

.txbox {
    position: absolute;
    top: 10%;
    left: 50%;
    -webkit-transform: translate(-50%, 0);
    transform: translate(-50%, 0);
    z-index: 5;
    text-align: center;
}

.txbox .img {
    width: 50px;
    -webkit-border-radius: 50%;
    -moz-border-radius: 50%;
    border-radius: 50%;
    overflow: hidden;
    margin: 0 auto;
}

.txbox .name {
    height: 25px;
    line-height: 25px;
    color: #fff;
    font-size: 18px;
    padding: 0 10px;
    background: #c20000;
    -webkit-border-radius: 50px;
    -moz-border-radius: 50px;
    border-radius: 50px;
    margin-top: 10px;
}

.navbox {
    position: absolute;
    right: 15px;
    z-index: 5;
    top: 40%;
}

.navbox .nav {
    margin-bottom: 10px;
    position: relative;
}

.navbox .nav img {
    width: 100%;
    display: block;
    width: 56px;
}

.navbox .nav .num {
    font-size: 7px;
    color: #62ad52;
    position: absolute;
    margin: 0;
    top: 43px;
    left: 10px;
    width: 40px;
    height: 15px;
    line-height: 15px;
    text-align: center;
}

.navbox .nav .nums {
    position: absolute;
    top: 0px;
    left: 0px;
    width: 15px;
    height: 15px;
    line-height: 15px;
    text-align: center;
    -webkit-border-radius: 50%;
    -moz-border-radius: 50%;
    border-radius: 50%;
    z-index: 10;
    border: solid 1px #81531e;
    background: #fff;
    color: #62ad52;
    font-style: inherit;
}

.navbox .nav .text {
    text-align: center;
    color: #ffffff;
    font-size: 14px;
    margin: 0;
    text-indent: 6px;
}

.treeimg {
    width: 150px;
    /*height: 225px;*/
    position: fixed;
    /* bottom: 265px; */
    bottom: 24%;
    left: 50%;
    -webkit-transform: translate(-50%, 0);
    transform: translate(-50%, 0);
    z-index: 5;
}
.shu {
    width: 100%;
    position: absolute;
    /* right: 50%;
    transform: translateX(-50%); */
    bottom: 2px;

    z-index: 5;
}
.shu img {
    width: 100%;
}
.treeimg>img {
    width: 100%;
    display: block;
}

.treeimg .yun {
    position: absolute;
    min-width: 100%;
    height: 300px;
    top: 50%;
    left: 50%;
    -webkit-transform: translate(-50%, -50%);
    transform: translate(-50%, -50%);
    z-index: 7;
}

.treeimg .caizhai {
    position: absolute;
    top: 30px;
    left: 50%;
    margin-left: 24px;
    width: 48px;
}

.treeimg .cao1 {
    position: absolute;
    right: -6px;
    bottom: 10px;
    width: 48px;
    z-index: 5;
}

.treeimg .cao2 {
    position: absolute;
    left: -12px;
    bottom: 10px;
    width: 51px;
    z-index: 5;
}

.img img {
    width: 100%;
    display: block;
}

.jiaoshui_am {
    position: absolute;
    right: 75px;
    top: 40%;
    z-index: 10;
    display: none;
    width: 56px;
}

#shui {
    position: absolute;
    right: 122px;
    top: 40%;
    z-index: 10;
    margin-top: 25px;
    display: none;
    width: 20px;
}

#shui img {
    width: 100%;
    display: block;
}


.jindutiao {
    position: fixed;
    left: 50%;
    /* bottom: 155px; */
    bottom: 11%;
    -webkit-transform: translate(-50%, 0);
    transform: translate(-50%, 0);
    z-index: 10;
}

.jindutext, .ymsqtext {
    position: fixed;
    left: 50%;
    -webkit-transform: translate(-50%, 0);
    transform: translate(-50%, 0);
    /* bottom: 205px; */
    bottom: 16.5%;
    height: 25px;
    width: 100%;
    line-height: 25px;
    text-align: center;
    color: #ffffff;
    z-index: 10;
    font-size: 16px;

}

.jindutiao_box {
    height: 20px;
    width: 300px;
    background: #48b7de;
    border: solid 5px #6ddbfc;
    -webkit-border-radius: 50px;
    -moz-border-radius: 50px;
    border-radius: 50px;
    overflow: hidden;
    position: relative;
}

.jindutiao_jd {
    background: url("img/jindutiaobg.png") center center;
    background-size: auto 100%;
    height: 20px;
    width: 300px;
    -webkit-border-radius: 50px;
    -moz-border-radius: 50px;
    border-radius: 50px;
    position: absolute;
    left: -10%;
}

.shifei {
    position: absolute;
    right: 150px;
    top: 40%;
    z-index: 10;
    display: none;
}

.feiliao {
    position: absolute;
    right: 269px;
    top: 40%;
    z-index: 10;
    margin-top: 64px;
    display: none;
    width: 40px;
}


.amintabg {
    position: fixed;
    width: 100%;
    height: 100%;
    top: 0;
    left: 0;
    z-index: 20;
    display: none;
}

.model_bg {
    position: fixed;
    width: 100%;
    height: 100%;
    top: 0;
    left: 0;
    background: rgba(0, 0, 0, 0.5);
    z-index: 20;
}

.shopbox {
    width: 80%;
    position: fixed;
    left: 50%;
    top: 60%;
    -webkit-transform: translate(-50%, -50%);
    transform: translate(-50%, -50%);
    background: #ffffff;
    z-index: 25;
    -webkit-border-radius: 25px;
    -moz-border-radius: 25px;
    border-radius: 25px;
    padding: 40px 10px 10px;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;

}

.shopbox_title {
    position: absolute;
    background: #c20000;
    color: #ffffff;
    font-size: 16px;
    text-align: center;
    top: 0;
    left: 50%;
    -webkit-transform: translate(-50%, 0);
    transform: translate(-50%, 0);
    height: 25px;
    line-height: 25px;
    padding: 0 40px;
    -webkit-border-radius: 5px 5px 30px 30px;
    -moz-border-radius: 5px 5px 30px 30px;
    border-radius: 5px 5px 30px 30px;
}

.shopbox .prev {
    position: absolute;
    left: 15px;
    top: 12px;
    width: 40px;
}

.shopbox .next {
    position: absolute;
    right: 15px;
    top: 12px;
    width: 40px;
}

.shopbox_nav {
    padding: 0 5px;
}

.clearfix:after {
    content: '';
    display: block;
    width: 100%;
    height: 0;
    clear: both;
}

.shopbox_nav .nav {
    float: left;
    display: block;
    width: 60px;
    height: 25px;
    line-height: 25px;
    background: #ffe7bb;
    color: #000000;
    margin-left: 10px;
    text-align: center;
    font-size: 16px;
}

.shopbox_nav .nav.active {
    background: #c20000;
    color: #ffffff;
}

.shopbox_main {
    border: solid 8px #c20000;
    -webkit-border-radius: 15px;
    -moz-border-radius: 15px;
    border-radius: 15px;
    padding: 5px;
}

.shopbox_main,
.shopbox_main * {
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
}

.shopbox_box {
    padding: 5px;
    float: left;
    width: 25%;
}

.shopbox_box .box {
    display: block;
    width: 100%;
    height: 100%;
    background: #f9e5e5;
    -webkit-border-radius: 10px;
    -moz-border-radius: 10px;
    border-radius: 10px;
    position: relative;
}

.shopbox_box .box img {
    width: 100%;
    display: block;
}

.shopbox_box .box .title {
    font-size: 12px;
    text-align: center;
    position: absolute;
    width: 100%;
    bottom: 0px;
    height: 15px;
    line-height: 15px;
}

.shopbox_box.active1 .box {
    position: relative;
    overflow: hidden;
}

.shopbox_box.active1 .box:before {
    content: '';
    display: block;
    width: 100%;
    height: 100%;
    background: url("img/yijieguo.png") no-repeat left top;
    background-size: 50px auto;
    z-index: 10;
    position: absolute;
    left: 0;
    top: 0;
}


.chutou {
    position: absolute;
    right: 120px;
    top: 40%;
    z-index: 10;
    margin-top: 110px;
    display: none;
    width: 57px;
}

.chosetreebox {
    width: 95%;
    position: fixed;
    left: 50%;
    top: 50%;
    -webkit-transform: translate(-50%, -50%);
    transform: translate(-50%, -50%);
    z-index: 25;
    -webkit-border-radius: 50px;
    -moz-border-radius: 50px;
    border-radius: 50px;
    padding: 40px 10px 10px;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    text-align: center;
}

.chosetreebox * {
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
}

.chosetreebox_title {
    color: #040000;
    font-size: 16px;
    text-align: center;
    margin-bottom: 15px;
}

.chosetreelist .chosetreeitem {
    padding: 5px;
    float: left;
    width: 50%;
}

.chosetreelist .box {
    padding: 5px;
}

.chosetreelist .box img {
    width: 100%;
    display: block;
}

.chosetreelist .chosetreeitem p {
    margin: 0;
    text-align: center;
    font-size: 12px;
    color: #ffffff;
}

.chosetreelist2 .swiper-slide {
    padding: 20px;
}

.chosetreelist2 img {
    width: 100%;
}

.chosetreelist2 .title {
    font-size: 14px;
    color: #040000;
    text-align: center;
    display: block;
}

.chosetree .model_bg {
    background: rgba(255, 255, 255, 0.6);
}

.chosetree .btn_title {
    font-size: 12px;
    color: #040000;
    text-align: center;
}

.chosetree .btn {
    font-size: 18px;
    color: #FFFFFF;
    font-weight: bold;
    display: inline-block;
    padding: 10px 30px;
    -webkit-border-radius: 50px;
    -moz-border-radius: 50px;
    border-radius: 50px;
    background: #f2b945;
    margin: 0 auto;
}

.chosetree_qr .model_bg {
    background: rgba(255, 255, 255, 0.6);
}

.chosetree_qr .btn_title {
    font-size: 12px;
    color: #040000;
    text-align: center;
    margin-top: 20px;
}

.chosetree_qr .btn_title img {
    display: inline-block;
    width: 15px;
    position: relative;
    top: 3px;
    margin-right: 10px;
}

.chosetree_qr .btn {
    font-size: 18px;
    color: #FFFFFF;
    font-weight: bold;
    display: inline-block;
    padding: 10px 30px;
    -webkit-border-radius: 50px;
    -moz-border-radius: 50px;
    border-radius: 50px;
    background: #f2b945;
    margin: 0 auto;
}

#treeimglist {
    position: absolute;
    left: 0;
    width: 100%;
    height: 30px;
    bottom: 10px;
    z-index: 10;
}

#treeimglist .swiper-slide img {
    width: 30px;
    height: 30px;
    display: inline-block;

}

.topshow {
    position: absolute;
    top: 70px;
    left: 15px;
    right: 15px;
    z-index: 10;
    height: 30px;
    overflow: hidden;
    border-radius: 15px;
}

.topshow marquee {
    background: rgba(255, 255, 255, 0.50);
    padding: 0 10px;
    color: #040000;
}

.topshow p {
    margin: 0;
    display: block;
    height: 30px;
    line-height: 30px;
}


.navbox_left {
    position: absolute;
    left: 15px;
    z-index: 5;
    top: 40%;
}

.navbox_left .nav {
    margin-bottom: 10px;
    position: relative;
}

.navbox_left .nav img {
    width: 100%;
    display: block;
    width: 56px;
}

.jifenzhuanqu {
    position: absolute;
    right: 15px;
    top: 110px;
    color: #cf7166;
    font-size: 15px;
    z-index: 10;
}

