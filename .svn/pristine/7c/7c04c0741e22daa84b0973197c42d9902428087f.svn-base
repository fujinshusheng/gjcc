.z-keywords__password{
  &:after,
  &:before{
    content: '';
    display: block;
    clear: both;
    width: 100%;
    height: 0;
  }
  border-top: solid 1px #dcdcdc;
  .z-numberkey{
    width: 33.3333%;
    float: left;
    font-size: 28px;
    font-weight: bold;
    height: 40px;
    line-height: 40px;
    color: #000;
    text-align: center;
    border-bottom: solid 1px #dcdcdc;

    &:nth-child(3n-1){
      border-left: solid 1px #dcdcdc;
    }
    &:nth-child(3n){
      border-left: solid 1px #dcdcdc;
    }
  }
}