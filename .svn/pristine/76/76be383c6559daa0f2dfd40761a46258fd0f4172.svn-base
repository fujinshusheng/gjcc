.wuliu-border-top {
  &:before {
    display: block;
    content: '';
    width: 100%;
    height: 3px;
    background: url("../image/kuaidi.jpg") repeat-x;
    background-size: auto 100%;
    transform: none;
    position: static;
  }
}

.wuliu-border-bottom {
  &:after {
    display: block;
    content: '';
    width: 100%;
    height: 3px;
    background: url("../image/kuaidi.jpg") repeat-x;
    background-size: auto 100%;
    transform: none;
    position: static;
  }
}

