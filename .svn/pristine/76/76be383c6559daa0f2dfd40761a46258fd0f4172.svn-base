/* 我的积分 */
.myjifen_header {
  background-color: #303030;
}

.myjifen_headertop {
  padding: 20px 0;
  background-color: #303030;
  position: relative;
  z-index: 1;

  .touxiang {
    width: 70px;
    height: 70px;
    -webkit-border-radius: 50%;
    -moz-border-radius: 50%;
    border-radius: 50%;
    margin-bottom: 15px;
  }

  .tip {
    background-color: rgba(26, 26, 26, 0.5);
    height: 30px;
    line-height: 30px;
    font-size: 14px;
    text-align: center;
    -webkit-border-radius: 50px;
    -moz-border-radius: 50px;
    border-radius: 50px;
    padding: 0 10px;
    color: #ffce3a;

    span {
      background-image: linear-gradient(to right, #fff6c2, #ffce3a);
      -webkit-background-clip: text;
      color: transparent;
    }
  }

  .flexbox {
    position: absolute;
    bottom: 70px;
    right: 0;

    div {
      background-color: #595959;
      color: #ffffff;
      height: 25px;
      line-height: 25px;
      margin-top: 10px;
      padding: 0 10px;
      -webkit-border-radius: 50px 0 0 50px;
      -moz-border-radius: 50px 0 0 50px;
      border-radius: 50px 0 0 50px;
    }
  }
}

