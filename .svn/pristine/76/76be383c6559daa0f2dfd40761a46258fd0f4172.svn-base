      }
    }
  }
}

.shop_fl_footer {
  div {
    height: 45px;
    line-height: 45px;
    text-align: center;
    color: #000;
    font-size: 18px;
  }

  .btn1 {
    background-color: #d2d2d2;
    color: @font-color-black;
  }

  .btn2 {
    background-color: #a61f24;
    color: #ffffff;
  }
}

.shop_fl_jiagebox {
  padding: 0 @plr;

  .text {
    height: 30px;
    line-height: 30px;
    -webkit-border-radius: 50px;
    -moz-border-radius: 50px;
    border-radius: 50px;
    text-align: center;
    background-color: #e5e5e5;
  }

  .text+.text {
    margin-left: 20px;
  }
}

