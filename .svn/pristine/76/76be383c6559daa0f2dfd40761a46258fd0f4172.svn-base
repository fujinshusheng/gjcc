/* 商品评价 */
.commit_nav {
  padding: @ptb;
  background-color: #ffffff;

  .nav {
    margin: 5px;
    text-align: center;
    background-color: #edd2d3;
    color: @font-color-black;
    height: 30px;
    line-height: 30px;
    -webkit-border-radius: 50px;
    -moz-border-radius: 50px;
    border-radius: 50px;

    .iconfont {
      display: none;
    }
  }

  .nav.selected {
    background-color: transparent;
    border: solid 1px @red;
    color: @red;

    .iconfont {
      display: inline;
    }
  }
}

.commit_list {
  margin-top: 10px;

  .item {
    padding: @ptb @plr;
    background-color: #ffffff;

    .name {
      font-size: 18px;
      color: @font-color-black;
    }

    .time {
      font-size: 14px;
      color: #5d5d5d;
    }
  }

  .commit_main {
    color: @font-color-black;
  }

  .commit_hf {
    padding: 10px;
    background-color: #eeeeee;
    margin: 20px auto 10px;
    position: relative;

    &:after {
      content: '';
      display: block;
      width: 0;
      height: 0;
      border-bottom: 10px solid #eeeeee;
      border-left: 10px solid transparent;
      border-right: 10px solid transparent;
      position: absolute;
      top: -10px;
      left: 10px;
    }
  }

  .commit_footer {

    .commit_zan {
      border: solid 1px #eeeeee;
      -webkit-border-radius: 50px;
      -moz-border-radius: 50px;
      border-radius: 50px;
      padding: 0 20px;

      .iconfont {
        margin-right: 5px;
      }
    }

    .commit_zan.active {
      color: @red;

      .iconfont {}

    }
  }

  .touxiang {
    width: 50px;
    height: 50px;
    -webkit-border-radius: 50%;
    -moz-border-radius: 50%;
    border-radius: 50%;
    margin-right: 10px;
  }

  .item+.item {
    border-top: solid 1px @bordercolor;
  }
}

.srarbox {
  text-align: center;
  padding: 15px;
  background-color: #ffffff;

  .iconfont {
    font-size: 28px;
    margin: 0 10px;
    color: #c3c3c3;

    &.light {
      color: #ffe44c;
    }
  }
}

.show_number {
  padding-left: 50px;
  padding-top: 20px;
}

.show_number li {
