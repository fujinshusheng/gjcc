@red: #a61f24;

@bordercolor: #e5e5e5;
@plr: 15px;
@ptb: 10px;

@font-color-black: #0e0e0e;

[v-cloak] {
  display: none !important;
}

.alert_bg {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background-color: rgba(0, 0, 0, 0.6);
  z-index: 1;
}

.bt1 {
  border-top: solid 1px #e5e5e5;
}

.bb1 {
  border-bottom: solid 1px #e5e5e5;
}

.bl1 {
  border-left: solid 1px #e5e5e5;
}

.br1 {
  border-right: solid 1px #e5e5e5;
}

.login_bg {
  background: url("../image/bg.jpg") no-repeat;
  background-size: 100% 100%;
}

.forgot_bg {
  background: url("../image/bg2.jpg") no-repeat;
  background-size: 100% 100%;
}

.register_bg {
  background: url("../image/bg3.jpg") no-repeat;
  background-size: 100% 100%;
}

.bangding_bg {
  background: url("../image/bg4.jpg") no-repeat;
  background-size: 100% 100%;
}

.logo {
  width: 80%;
  margin: 75px auto 0;

  img {
    display: block;
    width: 80px;
    margin: 0 auto;
  }
}

.dsfbox {
  margin-top: 2em;

  .text {
    padding: 0 20px;
  }

  .x {
    width: 100%;
    height: 1px;
    background-color: #ffffff;
  }
}

.bd_touxiang {
  width: 80px;
  height: 80px;
  margin: 20px auto;
  -webkit-border-radius: 50%;
  -moz-border-radius: 50%;
  border-radius: 50%;
}

.otherlogin {
  text-align: center;
  margin-bottom: 30px;

  .box {
    display: inline-block;
    margin: 0 50px;

    img {
      width: 35px;
      height: 35px;
    }
  }
}

.yzmbtn {
  background-color: rgba(255, 255, 255, 0.2);
  color: #ffffff;
  border-radius: 0;
  border: none;
}

.yzmbtn.active {
  background-color: rgb(255, 255, 255);
  color: #666666;
}

/* 首页 */
.banner {
  width: 100%;
  min-height: 140px;

  // background-color: pink;
  img {
    width: 100%;
    // display: block;
    font-size: 0;
  }

  .tbox {
    position: absolute;
    left: 0;
    bottom: 0;
    right: 100px;
    height: 30px;
    line-height: 30px;
    color: #ffffff;

    .title {
      height: 30px;
      line-height: 30px;
      text-align: center;
      padding: 0 10px;
      background-color: @red;
    }

    .text {
      height: 30px;
      line-height: 30px;
      padding: 0 10px;
      background-color: rgba(0, 0, 0, 0.4);
    }
  }

  .swiper-pagination {
    width: 100px;
    right: 0;
    left: auto;
    bottom: 5px;
    text-align: center;

    .swiper-pagination-bullet {
      background-color: #ffffff;
      opacity: 0.5;
    }

    .swiper-pagination-bullet-active {
      opacity: 1;
    }
  }
}

.index1box,
.index2box {
  text-align: center;

  .textbox {
    height: 115px;
    padding: @ptb 0;

    .box {
      display: block;
      width: 50px;
      margin: 0 auto;
    }

    p {
      display: inline-block;
      float: left;
      line-height: 1.2;
      font-size: 12px;
    }

    .text {
      width: 18px;
    }

    .title {
      width: 30px;
      font-size: 18px;
      font-weight: bold;
    }
  }

  img {
    width: 100%;
    display: block;
  }
}

.index1box {
  .zui-col:nth-child(2) {
    .textbox {
      border-left: solid 1px @bordercolor;
      border-right: solid 1px @bordercolor;
      border-bottom: solid 1px @bordercolor;
    }
  }
}

.index2box {
  .textbox {
    height: 135px;
  }

  .zui-col:nth-child(2) {
    .textbox {
      border-left: solid 1px @bordercolor;
    }
  }
}

