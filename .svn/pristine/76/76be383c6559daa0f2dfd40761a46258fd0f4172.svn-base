  float: left;
}

.atar_Show p {
  background: url(../image/stars2.png);
  left: 0;
  height: 21px;
  width: 134px;
}

.show_number li span {
  display: inline-block;
  line-height: 21px;
}

.shop_fl {
  .shop_flbox {
    position: fixed;
    width: 80%;
    left: 20%;
    top: 0;
    bottom: 0;
    z-index: 10;
    background-color: #ffffff;

    &>div {
      padding-top: 20px;
